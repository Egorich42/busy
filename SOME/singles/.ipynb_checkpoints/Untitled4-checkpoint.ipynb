    "# coding: utf8\n",
    "\n",
    "import xlwt\n",
    "\n",
    "# Создаем книку\n",
    "book = xlwt.Workbook('utf8')\n",
    "\n",
    "\n",
    "\n",
    "# Добавляем лист\n",
    "sheet = book.add_sheet('аренда')\n",
    "\n",
    "# Лист в положении \"альбом\"\n",
    "sheet.portrait = False\n",
    "\n",
    "# Масштабирование при печати\n",
    "sheet.set_print_scaling(85)\n",
    "\n",
    "# Сохраняем в файл\n",
    "book.save('filename.xls')"
   ]
